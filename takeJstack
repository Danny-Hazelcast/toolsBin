ssh -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no simulator@${ip} << EOF
	jps | grep Member.* | cut -d ' ' -f1 | xargs -L 1 -I% jstack % >> $ip-serverJstack.out
	jps | grep Client.* | cut -d ' ' -f1 | xargs -L 1 -I% jstack % >> $ip-clientJstack.out
EOF

forIpsIn agents.txt scpDown *serverJstack.out jstack
forIpsIn agents.txt scpDown *clientJstack.out jstack
